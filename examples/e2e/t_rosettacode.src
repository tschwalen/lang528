import "testutils.src"; 

function sum_of_squares(
    vec # array of integers
)
  let sum = 0;
  let idx = 0;
  while idx < vec.length()
    let n = vec[idx];
    sum += n * n;
    idx += 1;
  ..
  return sum;
..

function test_sum_of_squares()
    assert(sum_of_squares([]) == 0, "zero case");
    assert(sum_of_squares([3, 1, 4, 1, 5, 9]) == 133, "real case");
..

# run all tests
function main()
    test_sum_of_squares();
..